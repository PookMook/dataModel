{ 
    "name":"Datamodel" ,
    "version": 2,
    "builds": [{
        "src": "*.js",
        "use": "@now/node-server"
    }],
    "routes": [
        {
          "src": "/",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/",
          "dest": "/index.js",
          "methods": ["POST"]
        },
        {
          "src": "/json",
          "dest": "/json.js",
          "methods": ["GET"]
        },
        {
          "src": "/json",
          "dest": "/json.js",
          "methods": ["POST"]
        }
      ]

}