{ 
    "name":"PerfTest" ,
    "version": 2,
    "env": {
      "AWS_ACCESS_KEY_ID":"@aws_secret_id",
      "AWS_SECRET_ACCESS_KEY":"@add aws_secret_key",
      "AWS_REGION":"us-east-1"
    },
    "builds": [{
        "src": "*.js",
        "use": "@now/node-server"
    }],
    "routes": [
        {
          "src": "/express",
          "dest": "/express.js",
          "methods": ["GET"]
        },
        {
          "src": "/express",
          "dest": "/express.js",
          "methods": ["POST"]
        },
        {
            "src": "/express-light",
            "dest": "/express-light.js",
            "methods": ["GET"]
          },
          {
            "src": "/express-light",
            "dest": "/express-light.js",
            "methods": ["POST"]
          }
      ]

}